{
  "name": "voice-inpainting-frontend",
  "version": "1.0.0",
  "description": "Frontend testing for Voice Inpainting Application",
  "scripts": {
    "test": "python -m pytest tests/ -v",
    "test-frontend": "echo 'Frontend tests require opening tests/test_frontend.html in a browser'",
    "test-backend": "python -m pytest tests/test_*.py -v --tb=short",
